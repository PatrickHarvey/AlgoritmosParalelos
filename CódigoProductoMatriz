
#include <iostream>

using namespace std;


int main(int argc, char *argv[])
{
int size = 2;
int tam = 2;                                          // funciona hasta 835
int a[tam][tam], b[tam][tam], c[tam][tam];

for(int i=0;i<size;i++)
   for(int j=0;j<size;j++)
       cin >> a[i][j];

for(int i=0;i<size;i++)
   for(int j=0;j<size;j++)
       cin >> b[i][j];

for (int i = 0; i < size; i++)
    for (int j = 0; j < size; j++)
    {
    c[i][j]=0;
        for (int k = 0; k < size; k++)
             c[i][j]=c[i][j]+(a[i][k]*b[k][j]);
    }

 for(int i=0;i<size;i++)
      {
      if(i==size-1)
          cout << endl;
      for(int j=0;j<size;j++)
          cout << &c[i][j] << " ";
      }
  cout << endl;
  cout << &c[0][0] - &c[1][1] << endl;              //imprimo las direcciones en memoria para ver la diferencia entre ellas.

return 0;
}
